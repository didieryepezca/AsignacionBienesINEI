<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />

    <title>@ViewData["Title"] - Asignación de Bienes</title>
    <link rel="stylesheet" href="~/css/demo.min.css" />
    <link rel="stylesheet" href="~/css/tabler.min.css" />
    <link rel="stylesheet" href="~/css/tabler-flags.min.css" />
    <link rel="stylesheet" href="~/css/tabler-vendors.min.css" />

    <link rel="stylesheet" href="~/css/loading-spinner.css" />
    <!-- SweetAlert CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.css">
    <link rel="stylesheet" href="~/css/jquery-datatable/jquery.datatables.min.css" />

    <!-- jQuery UI Styles For Autocompleter TextBoxes-->
    <link rel="stylesheet" href="//code.jquery.com/ui/1.13.2/themes/base/jquery-ui.css">    
    <!-- jQuery v3.6.0 Core -->
    <script src="~/lib/jquery/dist/jquery-3.6.0.js"></script>
    <!-- jQuery UI Core For the Autocompleter TextBoxes-->
    <script src="https://code.jquery.com/ui/1.13.2/jquery-ui.js"></script>

    <!-- SweetAlert Core -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
    <!-- UI Core -->
    <script src="~/js/ui/tabler.min.js"></script>
    <!-- LitePicker -->
    <script src="~/js/ui/litepicker.js"></script>
    <!-- Select Combos -->
    <script src="~/js/ui/tom-select.base.min.js"></script>
    <script src="~/js/jquery-datatable/jquery.dataTables.min.js"></script>

</head>

<body class="antialiased">
    <div class="wrapper">
        <header class="navbar navbar-expand-md navbar-dark d-print-none">
            <div class="container-xl">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbar-menu">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <h1 class="navbar-brand navbar-brand-autodark d-none-navbar-horizontal pe-0 pe-md-3">
                    <a asp-area="" asp-controller="Home" asp-action="Index">
                        ASIGNACION DE BIENES
                    </a>
                </h1>

                <div class="navbar-nav flex-row order-md-last">
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </header>
        <div class="navbar-expand-md">
            <div class="collapse navbar-collapse" id="navbar-menu">
                <div class="navbar navbar-light">
                    <div class="container-xl">
                        <ul class="navbar-nav">

                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
                                    <span class="nav-link-icon d-md-none d-lg-inline-block">
                                        <!-- Download SVG icon from http://tabler-icons.io/i/home -->
                                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-devices-pc" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                            <path d="M3 5h6v14h-6z"></path>
                                            <path d="M12 9h10v7h-10z"></path>
                                            <path d="M14 19h6"></path>
                                            <path d="M17 16v3"></path>
                                            <path d="M6 13v.01"></path>
                                            <path d="M6 16v.01"></path>
                                        </svg>
                                    </span>
                                    <span class="nav-link-title">
                                        Bienes
                                    </span>
                                </a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Asignacion" asp-action="Lista">
                                    <span class="nav-link-icon d-md-none d-lg-inline-block">
                                        <!-- Download SVG icon from http://tabler-icons.io/i/home -->
                                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-clipboard-list" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                            <path d="M9 5h-2a2 2 0 0 0 -2 2v12a2 2 0 0 0 2 2h10a2 2 0 0 0 2 -2v-12a2 2 0 0 0 -2 -2h-2"></path>
                                            <path d="M9 3m0 2a2 2 0 0 1 2 -2h2a2 2 0 0 1 2 2v0a2 2 0 0 1 -2 2h-2a2 2 0 0 1 -2 -2z"></path>
                                            <path d="M9 12l.01 0"></path>
                                            <path d="M13 12l2 0"></path>
                                            <path d="M9 16l.01 0"></path>
                                            <path d="M13 16l2 0"></path>
                                        </svg>
                                    </span>
                                    <span class="nav-link-title">
                                        Asignaciones
                                    </span>
                                </a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link" asp-area="" asp-controller="Mantenimiento" asp-action="Index">
                                    <span class="nav-link-icon d-md-none d-lg-inline-block">
                                        <!-- Download SVG icon from http://tabler-icons.io/i/home -->
                                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-tool" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                            <path d="M7 10h3v-3l-3.5 -3.5a6 6 0 0 1 8 8l6 6a2 2 0 0 1 -3 3l-6 -6a6 6 0 0 1 -8 -8l3.5 3.5"></path>
                                        </svg>
                                    </span>
                                    <span class="nav-link-title">
                                        Mantenimientos
                                    </span>
                                </a>
                            </li>

                        </ul>
                    </div>
                </div>
            </div>
        </div>

        <div class="page-wrapper">
            <div id="overlay-spinner" class="loading loading--full-height" style="display:none"></div>
            @RenderBody()
            <footer class="footer footer-transparent d-print-none">
                <div class="container">
                    <div class="row text-center align-items-center flex-row-reverse">
                        <div class="col-lg-auto ms-lg-auto">
                            <ul class="list-inline list-inline-dots mb-0">
                                <li class="list-inline-item">
                                    <a href="#" target="_blank" class="link-secondary" rel="noopener">
                                        <!-- Download SVG icon from http://tabler-icons.io/i/heart -->
                                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-robot" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                            <path d="M7 7h10a2 2 0 0 1 2 2v1l1 1v3l-1 1v3a2 2 0 0 1 -2 2h-10a2 2 0 0 1 -2 -2v-3l-1 -1v-3l1 -1v-1a2 2 0 0 1 2 -2z"></path>
                                            <path d="M10 16h4"></path>
                                            <circle cx="8.5" cy="11.5" r=".5" fill="currentColor"></circle>
                                            <circle cx="15.5" cy="11.5" r=".5" fill="currentColor"></circle>
                                            <path d="M9 7l-1 -4"></path>
                                            <path d="M15 7l1 -4"></path>
                                        </svg>
                                        Bachilleres Niño & Huaripata
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="col-12 col-lg-auto mt-3 mt-lg-0">
                            <ul class="list-inline list-inline-dots mb-0">
                                <li class="list-inline-item">
                                    Copyright &copy; 2023
                                    <a href="." class="link-secondary">Asignación de Bienes E.I.R.L</a>
                                    - Todos los Derechos Reservados
                                </li>
                                <li class="list-inline-item">
                                    v1.0
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    <script type="text/javascript">
        $(document).ready(function () {
            const useropts = document.getElementsByClassName("dropdown-item");
            for (var i = 0; i < useropts.length; i++) {
                useropts[i].addEventListener('click', function () {
                    document.getElementById("overlay-spinner").removeAttribute("style");
                });
            }

            const menuops = document.getElementsByClassName("nav-link");
            for (var i = 0; i < menuops.length; i++) {
                menuops[i].addEventListener('click', function () {
                    document.getElementById("overlay-spinner").removeAttribute("style");
                });
            }
        });
    </script>

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>